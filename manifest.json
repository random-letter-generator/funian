{
  "manifest_version": 3,
  "name": "funian",
  "version": "1.0",
  "description": "extra fancy chrome extension by Ian and Funan",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["./scripts/content.js"]
    }
  ],
  "action": {
    "default_icon": "assets/zoof-icon-min.png"
  },
  "web_accessible_resources": [
    {
      "matches": ["<all_urls>"],
      "resources": ["assets/*.gif", "assets/*.png"]
    }
  ]
}
